{
  "name": "ai-strategic-brainstorm",
  "private": false,
  "version": "1.0.0",
  "description": "Expert-grade strategic ideation tool powered by multi-provider AI APIs",
  "author": "BoxPistols",
  "homepage": "https://ai-strategic-brainstorm.vercel.app",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "fix": "prettier --write \"src/**/*.{ts,tsx,css}\" \"api/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css}\" \"api/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules/.vite node_modules/.cache",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "lint-staged": {
    "*.{ts,tsx,css}": "prettier --write"
  },
  "dependencies": {
    "@remotion/cli": "^4.0.427",
    "@remotion/player": "4.0.427",
    "html2pdf.js": "^0.14.0",
    "intro.js": "^8.3.2",
    "intro.js-react": "^1.0.0",
    "lucide-react": "^0.460.0",
    "pptxgenjs": "^4.0.1",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "remotion": "4.0.427"
  },
  "devDependencies": {
    "@types/node": "^25.3.0",
    "@types/react": "^18.3.28",
    "@types/react-dom": "^18.3.7",
    "@vercel/node": "^5.6.6",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "postcss": "^8.4.49",
    "prettier": "^3.8.1",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.9.3",
    "vite": "^5.4.11",
    "vitest": "^4.0.18",
    "storybook": "^10.2.11",
    "@storybook/react-vite": "^10.2.11",
    "@storybook/addon-a11y": "^10.2.11",
    "@storybook/addon-docs": "^10.2.11"
  },
  "keywords": [
    "ai",
    "brainstorming",
    "strategy",
    "openai",
    "react",
    "vite"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BoxPistols/ai-strategic-brainstorm.git"
  }
}
